class Solution:
    def numIdenticalPairs(self, nums: List[int]) -> int:
        num_pairs=0
        for i   in  range   (len(nums)) :
            for j   in  range   (len(nums)) :
                if  nums[i] ==  nums[j] :
                    if  i   <   j   :
                        num_pairs   +=  1
        return  num_pairs
